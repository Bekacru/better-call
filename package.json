    "@types/supertest": "^6.0.3",
    "arktype": "^2.0.0",
    "body-parser": "^2.2.0",
    "bumpp": "^9.4.1",
    "express": "^5.1.0",
    "supertest": "^7.1.4",
    "tsdown": "^0.15.7",
    "type-fest": "^4.23.0",
    "typescript": "^5.6.0-beta",
