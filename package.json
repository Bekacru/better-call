    "vitest": "^2.0.4"
  },
  "dependencies": {
    "@better-fetch/fetch": "^1.1.4",
    "rou3": "^0.5.1",
    "zod": "^3.23.8",
    "set-cookie-parser": "^2.7.1",
    "uncrypto": "^0.1.3"
